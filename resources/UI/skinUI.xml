<?xml version="1.0" ?>
<ui>
	<text nameSkin="defaultSkin">
		<font>UI/font/font.fnt</font>
		<height>
			<value>16.0</value>
			<type>pixel</type>
		</height>
		<color>1.0 1.0 1.0</color>
	</text>

	<window nameSkin="defaultSkin">
		<skin>
			<image>UI/window.tga</image>
			<part zone="top">23</part>
			<part zone="bottom">3</part>
			<part zone="left">3</part>
			<part zone="right">24</part>
		</skin>
		<textSkin>defaultSkin</textSkin>
	</window>
	
	<button nameSkin="defaultSkin">
		<skin type="default">
			<image>UI/butDefault.tga</image>
			<part zone="top">2</part>
			<part zone="bottom">2</part>
			<part zone="left">1</part>
			<part zone="right">1</part>
		</skin>
		<skin type="focus">
			<image>UI/butFocus.tga</image>
			<part zone="top">2</part>
			<part zone="bottom">2</part>
			<part zone="left">1</part>
			<part zone="right">1</part>
		</skin>
		<skin type="click">
			<image>UI/butClick.tga</image>
			<part zone="top">2</part>
			<part zone="bottom">2</part>
			<part zone="left">1</part>
			<part zone="right">1</part>
		</skin>
		<textSkin>defaultSkin</textSkin>
	</button>
	
	<textBox nameSkin="defaultSkin">
		<skin type="default">
			<image>UI/textBoxDefault.tga</image>
			<part zone="top">2</part>
			<part zone="bottom">2</part>
			<part zone="left">2</part>
			<part zone="right">2</part>
		</skin>
		<skin type="focus">
			<image>UI/textBoxFocus.tga</image>
			<part zone="top">2</part>
			<part zone="bottom">2</part>
			<part zone="left">2</part>
			<part zone="right">2</part>
		</skin>
		<textSkin>defaultSkin</textSkin>
	</textBox>

	<slider nameSkin="defaultSkin">
		<buttonsTextSkin>defaultSkin</buttonsTextSkin>
		<valuesTextSkin>defaultSkin</valuesTextSkin>
		<leftButtonText>&lt;&lt;</leftButtonText>
		<rightButtonText>&gt;&gt;</rightButtonText>
	</slider>
</ui>
